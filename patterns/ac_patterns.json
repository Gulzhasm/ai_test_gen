      "canonical": "moves to lowest z-order",
      "category": "outcome",
      "subcategory": "z_order",
      "synonyms": [
        "appears below all",
        "rendered at bottom",
        "displayed behind",
        "moves to bottom layer",
        "goes to the back"
      ],
      "regex_fallback": "(?:move[s]?|appear[s]?)\\s+(?:to\\s+)?(?:the\\s+)?(lowest|bottom|below all)"
    },
    {
