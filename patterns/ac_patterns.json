      ],
      "regex_fallback": "(?:when|with|if)\\s+no\\s+(selection|object)"
    },
    {
      "id": "boundary_at_top",
      "canonical": "already at top",
      "category": "boundary",
      "subcategory": "position",
      "synonyms": [
        "at highest z-order",
        "already in front",
        "at top layer",
        "when at the top"
      ],
      "regex_fallback": "(?:at|when at|already at)\\s+(?:the\\s+)?(top|front|highest)"
    },
    {
      "id": "boundary_at_bottom",
      "canonical": "already at bottom",
      "category": "boundary",
      "subcategory": "position",
      "synonyms": [
        "at lowest z-order",
        "already at back",
        "at bottom layer",
        "when at the bottom"
      ],
      "regex_fallback": "(?:at|when at|already at)\\s+(?:the\\s+)?(bottom|back|lowest)"
    },
    {
