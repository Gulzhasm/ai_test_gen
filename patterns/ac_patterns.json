    {
      "id": "action_hide",
      "canonical": "hide",
      "category": "action",
      "subcategory": "visibility",
      "synonyms": [
        "conceal",
        "make invisible",
        "hide element",
        "remove from view"
      ]
    },
    {
      "id": "action_toggle",
      "canonical": "toggle",
      "category": "action",
      "subcategory": "state",
      "synonyms": [
        "switch",
        "toggle on off",
        "flip state",
        "toggle setting"
      ]
    },
    {
      "id": "action_select",
      "canonical": "select",
      "category": "action",
