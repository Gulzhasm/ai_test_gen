        "scale object",
        "change size",
        "make bigger",
        "make smaller",
        "enlarge",
        "shrink"
      ]
    },
    {
      "id": "action_enable",
      "canonical": "enable",
      "category": "action",
      "subcategory": "state",
      "synonyms": [
        "turn on",
        "activate",
        "make available",
        "switch on",
        "enable feature",
        "make active"
      ],
      "regex_fallback": "(enable|disable|toggle|show|hide)"
    },
    {
      "id": "action_disable",
      "canonical": "disable",
      "category": "action",
      "subcategory": "state",
      "synonyms": [
        "turn off",
        "deactivate",
        "make unavailable",
        "switch off",
