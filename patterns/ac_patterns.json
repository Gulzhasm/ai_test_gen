      "regex_fallback": "(?:increase[s]?|move[s]? up)\\s+(?:by\\s+)?one\\s+level"
    },
    {
      "id": "outcome_decreases_zorder",
      "canonical": "decreases z-order by one level",
      "category": "outcome",
      "subcategory": "z_order",
      "synonyms": [
        "moves down one level",
        "goes down in stack",
        "one layer lower"
      ],
      "regex_fallback": "(?:decrease[s]?|move[s]? down)\\s+(?:by\\s+)?one\\s+level"
    },
