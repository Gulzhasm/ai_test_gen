{
  "version": "1.0",
  "description": "Canonical AC patterns for semantic embedding matching. Migrated from ac_parser.py ACTION_PATTERNS, OUTCOME_PATTERNS, BOUNDARY_PATTERNS.",
  "patterns": [
    {
      "id": "action_bring_to_front",
      "canonical": "bring to front",
      "category": "action",
      "subcategory": "z_order",
      "synonyms": [
        "move to front",
        "bring above objects",
        "move above other objects",
        "place on top layer",
        "elevate z-order",
        "bring above all",
        "put in front of everything",
        "send to top"
      ],
      "regex_fallback": "(bring|move|send)\\s+(?:to\\s+)?(front|above)"
    },
    {
      "id": "action_send_to_back",
      "canonical": "send to back",
      "category": "action",
      "subcategory": "z_order",
      "synonyms": [
        "move to back",
        "send under objects",
        "move behind other objects",
        "place on bottom layer",
        "lower z-order",
        "send under all",
        "put behind everything"
      ],
      "regex_fallback": "(bring|move|send)\\s+(?:to\\s+)?(back|under)"
    },
    {
      "id": "action_rotate",
      "canonical": "rotate",
      "category": "action",
      "subcategory": "transform",
      "synonyms": [
        "rotate object",
        "spin",
        "turn",
        "rotate shape",
        "apply rotation"
      ],
      "regex_fallback": "(rotate|flip|mirror|transform|scale)"
    },
    {
      "id": "action_flip",
      "canonical": "flip",
      "category": "action",
      "subcategory": "transform",
      "synonyms": [
        "flip object",
        "mirror",
        "flip horizontally",
        "flip vertically",
        "mirror shape"
      ]
    },
    {
