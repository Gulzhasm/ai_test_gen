        "click button",
        "click on"
      ]
    },
    {
      "id": "action_create",
      "canonical": "create",
      "category": "action",
      "subcategory": "crud",
      "synonyms": [
        "add",
        "insert",
        "draw",
        "create new",
        "make new"
      ],
      "regex_fallback": "(add|remove|create|delete)"
    },
    {
      "id": "action_delete",
      "canonical": "delete",
      "category": "action",
      "subcategory": "crud",
      "synonyms": [
        "remove",
        "erase",
        "destroy",
        "delete item"
      ]
    },
    {
