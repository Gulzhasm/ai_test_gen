      "synonyms": [
        "choose",
        "pick",
        "click on",
        "select item",
        "highlight"
      ],
      "regex_fallback": "user\\s+(chooses?|selects?|clicks?|toggles?)"
    },
    {
      "id": "action_click",
      "canonical": "click",
      "category": "action",
      "subcategory": "user_action",
      "synonyms": [
        "press",
